<script>
function startTimer(duration, display) {
    var timer = duration, minutes, seconds;
if(duration==9999)
return false;
else
{    
    setInterval(function () {
        minutes = parseInt(timer / 60, 10);
        seconds = parseInt(timer % 60, 10);

        minutes = minutes < 10 ? "0" + minutes : minutes;
        seconds = seconds < 10 ? "0" + seconds : seconds;

        display.textContent = minutes + ":" + seconds;

        if (--timer < 0) {
           
di = document.querySelector('#boom');
di.textContent ="boommm";
var img = document.getElementById("image");
img.src="https://i2.wp.com/metro.co.uk/wp-content/uploads/2018/04/sei_5806423.jpg?quality=90&strip=all&zoom=1&resize=644%2C425&ssl=1";
return false;

        }
    }, 1000);

}}

window.onload = function () {
    var fiveMinutes = <%=countdown%>,
        display = document.querySelector('#time');
    startTimer(fiveMinutes, display);
};

</script>
